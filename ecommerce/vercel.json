{
    
    "builds": [
      {
        "src": "ecommerce/sgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ecommerce/wsgi.py"
      }
    ]
  }